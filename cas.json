// Root CA
{
  "privateKey": "1234567890",
  "commonName": "veriSign",
  "certificates": []
}

// Intermediate CA
{
  "privateKey": "1234567890",
  "commonName": "google",
  "certificates": [
    {
      "data": {
        "subjectName": {
          "commonName":"google"
        },
        "issuerName": {
          "commonName":"veriSign"
        },
        "publicKeyInfo": {
          "algorithm": "ecdsa-sha256",
          "keyUsage": "verify",
          "publicKey":"123213123123123"
        }
      },
      "signature": "signed by verisign"
    }
  ]
}


